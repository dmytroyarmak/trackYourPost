<div class="panel panel-info">
    <div class="panel-heading">
        <h2 class="panel-title">Додавання нового номеру</h2>
    </div>
    <div class="panel-body">
        <form class="row j-add-track-from" role="form">
            <div class="form-group col-sm-5">
                <label for="barcode-input" class="control-label">Номер відправлення</label>
                <input class="form-control j-barcode-input" id="barcode-input" placeholder="AB123456789CD">
            </div>
            <div class="form-group col-sm-5">
                <label for="description-input" class="control-label">Опис відправлення</label>
                <input class="form-control j-description-input" id="description-input">
            </div>
            <div class="form-group col-sm-2">
                <button type="submit" class="btn btn-primary btn-block btn-form-inline">Додати</button>
            </div>
        </form>
    </div>
</div>
